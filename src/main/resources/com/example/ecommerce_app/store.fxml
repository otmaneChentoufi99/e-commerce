<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.Insets?>
<BorderPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.ecommerce_app.controller.StoreController">
    <top>
        <HBox fx:id="navbar" spacing="20" alignment="CENTER_LEFT">
            <padding>
                <Insets top="10" right="20" bottom="10" left="20"/>
            </padding>
        </HBox>
    </top>
    <center>
        <ScrollPane fitToWidth="true">
                <FlowPane fx:id="productContainer" hgap="40" vgap="20">
                    <padding>
                        <Insets top="30" right="150" bottom="0" left="160"/>
                    </padding>
                </FlowPane>
    </ScrollPane>
    </center>
</BorderPane>
